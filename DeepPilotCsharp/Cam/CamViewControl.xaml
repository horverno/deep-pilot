<UserControl x:Class="DeepPilotCsharp.Cam.CamViewControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             xmlns:local="clr-namespace:DeepPilotCsharp.Cam"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <WindowsFormsHost Name="CamCanvasHost" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <WindowsFormsHost.Child>
                <wf:PictureBox Name="CamCanvas" Dock="Fill"/>
            </WindowsFormsHost.Child>
        </WindowsFormsHost>
    </Grid>
</UserControl>
